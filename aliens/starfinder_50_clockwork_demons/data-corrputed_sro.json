        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "A data-corrupted creature is capable of infecting certain victims with a self-replicating virus that gradually alters their minds. When a data-corrupted creature deals damage with their claw attack to a non-mindless technological construct or a living creature with the android subtype, the target must succeed at a DC 18 Will saving throw or be infected with the data infection curse (see below).",
                "name": "Corrupting Virus",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "As a reaction to being targeted by an attack that targets EAC, a data-corrupted creature can create a temporary shield of corrupted data that grants a +2 bonus to EAC against that attack.",
                "name": "Firewall",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su"
            }
        ],
