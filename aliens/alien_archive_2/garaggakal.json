        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "As a standard action, a garaggakal can spend 1 Resolve Point to leech the life from one target within 30 feet. This deals 3d6 damage (Fortitude DC 13 half), and the garaggakal gains temporary Hit Points equal to the amount of damage dealt.",
                "name": "Leech Life",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "A garaggakal can momentarily shift out of phase with reality. As a full action, a garaggakal can spend 1 Resolve Point to phase through up to 15 feet of solid matter. If it attempts to phase through something that is more than 15 feet thick, the attempt fails, but the action and Resolve Points are still expended. A garaggakal cannot phase through force effects, such as force fields.",
                "name": "Phase Through",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su"
            }
        ]
