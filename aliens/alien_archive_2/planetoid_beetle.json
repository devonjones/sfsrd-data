        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "Once per round as a standard action, the planetoid beetle can spit acid as a ranged attack that has the line property and a range of 60 feet.",
                "name": "Acid Spit",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "When attacking an object, a planetoid beetle treats that object\u2019s hardness as 10 lower.",
                "name": "Break Objects",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "A planetoid beetle can eat objects with a hardness of 10 or lower. It deals 4 damage per minute to an object it\u2019s consuming. It is sated and stops feeding on a given day after dealing 400 damage in this way.",
                "name": "Consume Objects",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "Using its acid to soften rock, a planetoid beetle can burrow through stone at half speed. It can make tunnels when it burrows.",
                "name": "Rock Tunneler",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            }
