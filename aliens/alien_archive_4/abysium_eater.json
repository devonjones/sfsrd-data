        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "A creature hit by an abysium eater\u2019s slam attack takes a \u20131 penalty to its next saving throw against radiation sickness attempted within the next 24 hours. This penalty stacks, to a maximum equal to half the abysium eater\u2019s CR (\u20132 for a typical specimen).",
                "name": "Fallout Susceptibility",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "An abysium eater emanates medium radiation in a 20-foot radius. This radiation suffuses an area in 20-foot increments. An abysium eater is considered a natural radioactive material for the purposes of spells that remove radiation, such as <i>remove radioactivity</i>.",
                "name": "Irradiation",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "When an abysium eater critically hits a creature wearing armor that grants immunity to radiation levels, the target loses that radiation immunity (Reflex DC 13 negates), though the armor continues to provide its saving throw bonus against radiation as normal. This damage persists until the armor is repaired.",
                "name": "Shear Shielding",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
            }
        ]
