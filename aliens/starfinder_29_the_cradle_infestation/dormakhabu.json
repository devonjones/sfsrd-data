            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "As a ranged attack, a dormakhabu can spit a thin stream of acid from its mouth with a range increment of 30 feet.",
                "name": "Acidic Spit",
                "ability_type": "Extraordinary",
                "ability_type_abbrev": "Ex"
