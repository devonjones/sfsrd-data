        "special_abilities": [
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "Once per day as a swift action, a svartalfar can imbue one of its weapons with the bane weapon fusion. It must select one creature type (and subtype, if choosing humanoid or outsider) when it uses this ability. This lasts for 1 hour. This ability functions only while the svartalfar wields the weapon.",
                "name": "Bane",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su"
            },
            {
                "type": "stat_block_section",
                "subtype": "special_ability",
                "text": "As a swift action after hitting with a melee weapon, a svartalfar can cast and deliver its entropic grasp spell-like ability to affect the target\u2019s armor, if it hasn\u2019t already cast it yet that day. If the spell is cast in this way, its duration is an additional 6 rounds.",
                "name": "Spell Strike",
                "ability_type": "Supernatural",
                "ability_type_abbrev": "Su"
            }
